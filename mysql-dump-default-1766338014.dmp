INSERT INTO `categories` VALUES ('cmhfvlpem0008s10kjml5l9m2','Men Shawls','men-shawls',NULL,'/uploads/87acbc6d-9b6a-4afe-b596-a67c371d2731-1.avif',NULL,'2025-11-01 06:03:03.262','2025-11-03 09:15:51.866',NULL,'[{\"title\":\"MEN SHAWLS\",\"description\":\"Rooted in the heritage of Pashtun tradition, the Men’s Shawl by KhyberWear represents a blend of cultural pride and timeless sophistication. Each shawl is handwoven from premium Charsadda khaddar, using age-old looms that preserve the authentic craft of Swat and Charsadda artisans. The fabric’s refined texture and natural warmth make it ideal for both winter comfort and ceremonial elegance.\\r\\nDesigned with a bold border weave and balanced drape, this traditional lohi complements both modern tailoring and ethnic attire. Whether you’re attending a formal gathering or embracing everyday sophistication, KhyberWear’s Men Shawl embodies confidence, heritage, and enduring class — an heirloom of craftsmanship that transcends trends.\",\"image\":{\"url\":\"/uploads/2-scaled.jpg\",\"alt\":\"\"}},{\"title\":\"Discover the Latest in Men Shawls\",\"description\":\"Experience refined elegance with KhyberWear’s Maroon Men’s Shawl, a modern expression of Pashtun craftsmanship woven in premium Charsadda khaddar. Its rich maroon hue and hand-finished borders reflect the perfect harmony between heritage design and contemporary style. The shawl’s soft texture, natural drape, and breathable weave make it versatile — equally suited for formal events, winter wear, or cultural celebrations.\\r\\nEvery fold tells a story of confidence, culture, and enduring grace, carrying forward the legacy of handmade khaddar weaving from Pakistan’s heartland. A true statement of refined taste, it allows today’s gentleman to celebrate tradition with sophistication — effortlessly blending heritage with modern sensibility.\",\"image\":{\"url\":\"/uploads/2-scaled.jpg\",\"alt\":\"\"}},{\"title\":\"Why Choose KhyberWear?\",\"description\":\"Why Choose KhyberWear?\\r\\nAt KhyberWear, tradition isn’t just a fashion statement — it’s a living legacy. Our artisans from Charsadda and Swat uphold generations of Pashtun weaving heritage, crafting products that unite authenticity and modern refinement. From premium shawls to classic pakols and handcrafted Peshawari footwear, every piece is made with integrity and cultural respect.\\r\\nWe use only locally sourced khaddar and natural wool, ensuring comfort, durability, and sustainable craftsmanship. Whether you’re dressing for heritage occasions or urban sophistication, KhyberWear helps you embody confidence, culture, and class — in every layer, weave, and stitch.\",\"image\":{\"url\":\"/uploads/2-scaled.jpg\",\"alt\":\"\"}}]',NULL,NULL,NULL),('cmhfxb8bl0000o6012lyzmbvp','Kids Shawls','kids-shawls',NULL,'/uploads/ADI00721.webp',NULL,'2025-11-01 06:50:53.793','2025-11-01 17:38:24.895',NULL,NULL,NULL,NULL,NULL),('cmhgjsomn0000ph0197m3g3ux','Women Shawls','women-shals',NULL,'/uploads/11.jpg',NULL,'2025-11-01 17:20:19.631','2025-12-04 08:02:18.796',NULL,NULL,NULL,NULL,NULL),('cmieixoc9001npm01nin5jz4x','Charsadda Khaddar','charsaddakhaddar',NULL,'/uploads/3.jpg','CharsaddaKhaddar','2025-11-25 12:00:22.903','2025-11-28 18:18:24.381',NULL,NULL,NULL,NULL,NULL);
